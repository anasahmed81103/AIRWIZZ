@{
    ViewData["Title"] = "Home Page";
}


	<section id="home" class="welcome-hero">

		<!-- top-area Start -->
		
		<!-- top-area End -->

		<div class="container">
			<div class="welcome-hero-txt">
				<h2>get a flight to your dream destination today!</h2>
				<p>
				Explore a wide range of flight options tailored to your preferences. Choose your departure and arrival locations, select dates, and let us help you find the best deals.
				</p>
				<button class="welcome-btn" onclick="window.location.href='#'">contact us</button>
			</div>
		</div>

	<div class="container">
		<div class="row">
			<div class="col-md-12">
				

				<form class="model-search-content" action="search_results.html" method="GET">


					<div class="row">


						<div class="col-md-offset-1 col-md-2 col-sm-12">
							<div class="single-model-search">
								<h2>Select Travel Date</h2>
								<div class="model-select-icon">
									<input type="date" class="form-control" placeholder="Select Date" name="travel_date" required>
								</div><!-- /.model-select-icon -->
							</div>
							<div class="single-model-search">
								<h2>Flight Class</h2>
								<div class="model-select-icon">
									<select class="form-control" name="flight_class" required>
										<option value="">Select Class</option>
										<option value="economy">Economy</option>
										<option value="business">Business</option>
										<option value="first">First Class</option>
									</select><!-- /.select-->
								</div><!-- /.model-select-icon -->
							</div>
						</div>


						<div class="col-md-offset-1 col-md-2 col-sm-12">
							<div class="single-model-search">
								<h2>Departure Location</h2>
								<div class="model-select-icon">
									<select class="form-control" name="departure_location" required>
										<option value="">Select Location</option>
										<option value="karachi">Karachi</option>
										<option value="lahore">Lahore</option>
										<option value="islamabad">Islamabad</option>
										<option value="peshawar">Peshawar</option>
										<option value="quetta">Quetta</option>
										<option value="abu-dhabi">Abu Dhabi</option>
										<option value="doha">Doha</option>
										<option value="riyadh">Riyadh</option>
										<option value="dubai">Dubai</option>
										<option value="manama">Bahrain</option>
										<option value="muscat">Muscat</option>
									</select><!-- /.select-->
								</div><!-- /.model-select-icon -->
							</div>
							<div class="single-model-search">
								<h2>Return Option</h2>
								<div class="model-select-icon">
									<select class="form-control" name="arrival_location" required>
										<option value="">Select Location</option>
										<option value="lahore">Lahore</option>
										<option value="karachi">Karachi</option>
										<option value="islamabad">Islamabad</option>
										<option value="doha">Doha</option>
										<option value="dubai">Dubai</option>
										<option value="riyadh">Riyadh</option>
										<option value="abudhabi">Abu Dhabi</option>
										<option value="istanbul">Istanbul</option>
										<option value="jeddah">Jeddah</option>
										<option value="melbourne">Melbourne</option>
										<option value="sydney">Sydney</option>
										<option value="new-york">New York</option>
										<option value="los-angeles">Los Angeles</option>
										<option value="london">London</option>
										<option value="paris">Paris</option>
										<option value="berlin">Berlin</option>
									</select><!-- /.select-->
								</div><!-- /.model-select-icon -->
							</div>
						</div>


						<div class="col-md-offset-1 col-md-2 col-sm-12">
							<div class="single-model-search">
								<h2>Arrival Location</h2>
								<div class="model-select-icon">
									<select class="form-control" name="arrival_location" required>
										<option value="">Select Location</option>
										<option value="miami">Miami</option>
										<option value="orlando">Orlando</option>
										<option value="las-vegas">Las Vegas</option>
									</select><!-- /.select-->
								</div><!-- /.model-select-icon -->
							</div>
							<div class="single-model-search">
								<h2>Price Range</h2>
								<div class="model-select-icon">
									<select class="form-control" name="price_range" required>
										<option value="">Select Price Range</option>
										<option value="$100-$500">$100 - $500</option>
										<option value="$500-$1000">$500 - $1000</option>
										<option value="$1000+">$1000+</option>
									</select><!-- /.select-->
								</div><!-- /.model-select-icon -->
							</div>
						</div>


						<div class="col-md-2 col-sm-12">
							<div class="single-model-search text-center">
								<button class="welcome-btn model-search-btn" type="submit">
									Search Flights
								</button>
							</div>
						</div>
					</div>


				</form>


			</div>
		</div>
	</div>


	</section><!--/.welcome-hero-->
	<!--welcome-hero end -->
	<!--service start -->



<section id="service" class="service" style="background-color:aliceblue;">
	<div class="container">
		<div class="service-content">
			<div class="row">
				<div class="col-md-4 col-sm-6">
					<div class="single-service-item">
						<div class="single-service-icon">
							<i class="flaticon-airplane"></i>
						</div>
						<h2><a href="#">Extensive Flight Options</a></h2>
						<p>
							We offer a wide range of flights to South Asia, the Middle East, Australia, Europe, and the USA to meet all your travel needs.
						</p>
					</div>
				</div>
				<div class="col-md-4 col-sm-6">
					<div class="single-service-item">
						<div class="single-service-icon">
							<i class="flaticon-boarding-pass"></i>
						</div>
						<h2><a href="#">Hassle-Free Booking</a></h2>
						<p>
							Enjoy a seamless booking experience with our user-friendly platform, ensuring you get the best deals on flights.
						</p>
					</div>
				</div>
				<div class="col-md-4 col-sm-6">
					<div class="single-service-item">
						<div class="single-service-icon">
							<i class="flaticon-support"></i>
						</div>
						<h2><a href="#">24/7 Customer Support</a></h2>
						<p>
							Our dedicated customer support team is available around the clock to assist you with any inquiries or issues.
						</p>
					</div>
				</div>
			</div>
		</div>
	</div><!--/.container-->

</section><!--/.service-->
<!--service end-->



